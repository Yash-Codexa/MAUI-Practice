<?xml version="1.0" encoding="utf-8" ?>
<Shell xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PagesDemo.DevsPark_MVVM.Pages.DevsPark_AppShell"
             xmlns:nightAdventures="clr-namespace:PagesDemo.DevsPark_MVVM.Pages.DesertMirage.NightAdventures"
             xmlns:oasisFun="clr-namespace:PagesDemo.DevsPark_MVVM.Pages.DesertMirage.OasisFun"
             xmlns:icyTreats="clr-namespace:PagesDemo.DevsPark_MVVM.Pages.Frozen.IcyTreats"
             xmlns:snowActivities="clr-namespace:PagesDemo.DevsPark_MVVM.Pages.Frozen.SnowActivities"
             xmlns:beachAttractions="clr-namespace:PagesDemo.DevsPark_MVVM.Pages.TropicalParadise.BeachAttractions"
             xmlns:jungleAdventures="clr-namespace:PagesDemo.DevsPark_MVVM.Pages.TropicalParadise.JungleAdventures"
             xmlns:local="clr-namespace:PagesDemo"
             xmlns:utilities="clr-namespace:PagesDemo.Utilities"
             >

        <ShellContent Title="Night Dune Buggy" ContentTemplate="{DataTemplate nightAdventures:NightDuneBuggy}" >
                <ShellContent.Icon>
                        <FontImageSource FontFamily="Icons" Color="{StaticResource IconColor}" Glyph="{x:Static utilities:DevPark_IconFont.Night_dune_buggy}"  />
                </ShellContent.Icon>
        </ShellContent>

        <ShellContent Title="Star Gazing" ContentTemplate="{DataTemplate nightAdventures:StarGazing}" >
                <ShellContent.Icon>
                        <FontImageSource FontFamily="Icons" Color="{StaticResource IconColor}" Glyph="{x:Static utilities:DevPark_IconFont.Star_gazing}"  />
                </ShellContent.Icon>
        </ShellContent>

